   "source": [
    "# -*- coding: UTF-8 -*-\n",
    "a=float(input('输入三角形第一边长：'))\n",
    "b=float(input('输入三角形第二边长：'))\n",
    "c=float(input('输入三角形第三边长：'))\n",
    "\n",
    "#计算三角形半周长\n",
    "s=(a+b+c)/2\n",
    "\n",
    "#计算面积\n",
    "area=(s*(s-a)*(s-b)*(s-c))**0.5\n",
    "print('三角形的面积为%0.2f'% area)\n"
   ]
